<form id="create-new-test">
	<label>{{t "Name"}}</label>
	<input name="post_title" required="true">
	<ul>
		{{#each kwpsTestModi}}
		<li>
			<input type="radio" value="{{ID}}" name="post_parent" id="kwpsTestModi_{{ID}}" required="true">
            <label for="kwpsTestModi_{{ID}}">{{post_title}}</label>
		</li>
		{{/each}}

	</ul>
	<div>
		<button type="submit">{{t "Create"}}</button>
	</div>
</form>
